#include <iostream>
#include <conio.h>

using namespace std;

int main()
{
	int x;
	
	cout << "Counting...\n";
	x=1;
	while (x<=10)
	{
		cout << x << ",";
		x++;
	}
	
	x=12;
	while (x<=30)
	{
		cout << x << ",";
		x=x+2;
		
		if (x==30) 
		{
			cout << "30";
			break;
		}
		
	}
	
	
	_getch();
	return 0;
	
}
